{
  "name": "persistence",
  "version": "1.0.7",
  "description": "An abstraction library for redis and sentinel connection management",
  "main": "lib/index.js",
  "scripts": {
    "check-modules": "if [ -z \"$SKIP_PACKAGE_CHECK\" ] && [ ./package.json -nt ./node_modules ]; then echo 'Your node_modules are out of date. Please run \"npm update\"' && exit 1; fi",
    "pretest": "npm run check-modules",
    "test-redis": "ls ./test/*.test.js | xargs -n 1 -t -I {} sh -c 'TEST=\"{}\" npm run test-one'",
    "test": "npm run test-redis",
    "test-one": "./node_modules/.bin/mocha --ui exports --reporter spec --slow 2000ms --timeout 4000ms --bail \"$TEST\"",
    "test-one-solo": "./node_modules/.bin/mocha --ui exports --reporter spec --slow 2000ms --timeout 4000ms --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zendesk/persistence.git"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "3.2.0",
    "proxyquire": "1.7.3",
    "simple_sentinel": "0.1.8",
    "json3": "3.3.2",
    "lodash.create": "3.1.1",
    "callback_tracker": "0.10.0",
    "minilog": "3.1.10",
    "debug": "2.2.0",
    "diff": "1.4.0",
    "glob": "7.0.5"
  },
  "dependencies": {
    "redis": "0.12.1",
    "redis-sentinel": "zendesk/node-redis-sentinel.git"
  }
}
